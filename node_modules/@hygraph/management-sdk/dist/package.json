{
    "name": "@hygraph/management-sdk",
    "version": "1.2.5",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "license": "MIT",
    "files": [
        "dist",
        "!dist/**/*.*.test.js"
    ],
    "devDependencies": {
        "@ava/typescript": "^3.0.1",
        "@types/node": "^18.0.4",
        "ava": "^4.3.1",
        "nock": "^13.2.8",
        "typedoc": "^0.23.21",
        "typescript": "^4.7.4"
    },
    "scripts": {
        "test": "rm -rf dist && tsc && ava --serial",
        "build": "rm -rf dist && tsc",
        "prepublish": "rm -rf dist && rm -rf docs && tsc &&  npx typedoc --out docs src"
    },
    "dependencies": {
        "graphql": "^16.5.0",
        "graphql-request": "^4.3.0"
    },
    "ava": {
        "files": [
            "src/**/*.test.ts"
        ],
        "typescript": {
            "rewritePaths": {
                "src/": "dist/"
            },
            "compile": false
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
